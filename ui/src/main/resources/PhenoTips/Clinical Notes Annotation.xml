    <property>
      <code>var generateSuggestion = function(target, text, start, end) {&#xd;
  target.insert(&#xd;
    new Element('div', {'class' : 'suggestion'}).insert(&#xd;
      new Element('div', {'class' : 'suggestion-term'}).update(text.substring(start, end))&#xd;
    ).insert(&#xd;
      new Element('div', {'class' : 'suggestion-quote'}).insert(&#xd;
        text.substring(0, start) &#xd;
      ).insert(&#xd;
        new Element('b', {'class' : 'quoted-term'}).update(text.substring(start, end))&#xd;
      ).insert(&#xd;
        text.substring(end)&#xd;
      )&#xd;
    ).insert(&#xd;
      new Element('div', {'class' : 'hide-suggestion'}).update("âœ–")&#xd;
    )&#xd;
  );&#xd;
}&#xd;
&#xd;
var tempText = "Johnathan ate seven apples using his blue eyes and ...";&#xd;
var start = tempText.indexOf("blue eyes");&#xd;
var end = start + "blue eyes".length;&#xd;
var allSuggestions = [];&#xd;
&#xd;
for(var i = 0; i &lt; 10; i++) {&#xd;
  allSuggestions.push({'text' : tempText, 'start' : start, 'end' : end});&#xd;
}&#xd;
&#xd;
var currentSuggestions = [allSuggestions.shift(), allSuggestions.shift(), allSuggestions.shift(), allSuggestions.shift(), allSuggestions.shift(), allSuggestions.shift()];&#xd;
&#xd;
&#xd;
&#xd;
var clinicalNotesSubPanel = &#xd;
  new Element('div', {"class" : "sub-panel"}).insert(&#xd;
    new Element('h3', {"class" : "wikigeneratedheader"}).update(&#xd;
      "&lt;span>&lt;strong>Suggestions from Clinical Notes&lt;/strong>&lt;/span>"&#xd;
    )&#xd;
  );&#xd;
&#xd;
var suggestionsContainer = new Element('div', {'id' : 'suggestions-container'});&#xd;
currentSuggestions.forEach(function(item) {&#xd;
  generateSuggestion(suggestionsContainer, item.text, item.start, item.end);&#xd;
});&#xd;
&#xd;
clinicalNotesSubPanel.insert(suggestionsContainer);&#xd;
$$(".current-phenotype-selection")[0].insert({top : clinicalNotesSubPanel});</code>
